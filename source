-- Gui to Lua
-- Version: 3.2

-- Instances:

if not game:IsLoaded() then
game.Loaded:Wait()
end

local Hub = game.Players:FindFirstChild("SixHub")
if Hub == nil then
    loadstring(game:HttpGet('https://raw.githubusercontent.com/sicksixics/SixHub/main/source'))()
else
    warn'Already open!'
end

game:GetService("StarterGui"):SetCore("SendNotification", {
Title = "SixHub";
Text = "Open with M";
})
wait()

game:GetService("StarterGui"):SetCore("SendNotification", {
Title = "Made By:";
Text = "sicksixics & ItsAll4n";
})
wait(1)
